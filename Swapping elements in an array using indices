
#include <stdio.h>

int main()
{
    int n;
    printf("Enter the value of n:");
    scanf("%d",&n);
    printf("enter %d elements",n);
    
    
    int arr[n];
    for (int i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    
    
    int i1,i2;
    scanf("%d %d",&i1,&i2);
    
    
    if(i1>=0 && i1<n && i2>=0 && i2<n){
        int temp=arr[i1];
        arr[i1]=arr[i2];
        arr[i2]=temp;

        printf("after swapping:\n");
        for (int i=0; i<n; i++){
            printf("%d ", arr[i]);
        }
    }
    
    
    else{
            printf("Invalid indices");
    }
    
    return 0;
}
